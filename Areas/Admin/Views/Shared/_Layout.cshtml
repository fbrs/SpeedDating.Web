<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SpeedDating.Web</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/lib/AdminLTE/css/adminlte.min.css" />
    <link rel="stylesheet" href="~/lib/font-awesome-4.7/css/font-awesome.min.css" />
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <link rel="stylesheet" href="~/css/mainStyle.css" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
</head>
<body class="sidebar-mini" style="height: auto;">
    <div class="wrapper">
        @await Html.PartialAsync("_NavBar")
        @await Html.PartialAsync("_SideMenu")

        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            
            <!-- /.content-header -->
            <!-- Main content -->
            <section class="content">
                <div class="container-fluid">
                    <main role="main" class="pb-3">
                        @RenderBody()
                    </main>
                </div>
            </section>

            <!-- /.content -->

        </div>
        <aside class="control-sidebar control-sidebar-dark">
            <div class="p-3">
                <h5>Title</h5>
                <p>Sidebar content</p>
            </div>
        </aside>
        @await Html.PartialAsync("_Footer")
    </div>
    
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/AdminLTE/js/adminlte.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
